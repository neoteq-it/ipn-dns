  }
  template IN CAA ipn.neoteq.be {
    answer "ipn.neoteq.be. CAA 0 issue letsencrypt.org"
    fallthrough
  }
  azure STMN-Netzwerk:ipn.neoteq.be {
    tenant {$AZ_TENANT}
    client {$AZ_CLIENT}
    secret {$AZ_SECRET}
    subscription {$AZ_SUBSCRIPTION}
    fallthrough
  }
  tailscale ipn.neoteq.be {
    authkey {$TS_AUTHKEY}
    fallthrough
  }
  neoteqts4via6 {
    fallthrough
  }
  forward . 127.0.0.1:5353
}

